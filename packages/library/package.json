{
  "name": "@postiva/client",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.35",
  "keywords": [
    "react",
    "vue",
    "typescript",
    "solid",
    "postiva",
    "content",
    "api",
    "client",
    "cms",
    "nuxt-cms",
    "content-management",
    "headless-cms",
    "content-api"
  ],
  "description": "A comprehensive library for managing and retrieving content within your applications, @postiva/client streamlines your interaction with the Postiva platform, ensuring your content is always current and easily accessible.",
  "files": [
    "README.md",
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./src/index.ts",
  "module": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@types/jest": "~29.5",
    "@types/node": "~20",
    "@typescript-eslint/eslint-plugin": "~6.15",
    "@typescript-eslint/parser": "~6.15",
    "eslint": "~8.56",
    "eslint-config-prettier": "~9.1",
    "eslint-plugin-jest": "~27.6",
    "jest": "~29.7",
    "prettier": "~3.1",
    "rimraf": "~5.0",
    "ts-api-utils": "~1.0",
    "ts-jest": "~29.1",
    "tslib": "^2.6.3",
    "tsup": "^8.1.0",
    "typescript": "~5.3"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf coverage build tmp",
    "test": "jest --coverage",
    "prettier": "prettier --config .prettierrc --write .",
    "test:watch": "jest --watch"
  },
  "author": {
    "name": "Postiva",
    "email": "hello@postiva.app"
  },
  "license": "Apache-2.0",
  "volta": {
    "node": "20.10.0"
  }
}
